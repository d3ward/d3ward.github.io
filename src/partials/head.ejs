<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="theme-color" content="#161625">
	<% if( page == "404"){%>
		<link rel="icon" href="https://d3ward.github.io/assets/404/404.svg" type="image/svg+xml">
		<link rel="icon" type="image/png" sizes="16x16" href="https://d3ward.github.io/assets/404/favicon-16x16.png">
		<link rel="shortcut icon" href="https://d3ward.github.io/assets/favicon.ico">
		<link rel="apple-touch-icon" sizes="180x180" href="https://d3ward.github.io/assets/404/apple-touch-icon.png">
		<link rel="mask-icon" href="https://d3ward.github.io/assets/404/404.svg" color="#0074d9">
	<% }else{ %>
		<link rel="icon" href="https://d3ward.github.io/assets/icon.svg" type="image/svg+xml">
		<link rel="icon" type="image/png" sizes="16x16" href="https://d3ward.github.io/assets/favicon-16x16.png">
		<link rel="apple-touch-icon" sizes="180x180" href="https://d3ward.github.io/assets/apple-touch-icon.png">
		<link rel="mask-icon" href="https://d3ward.github.io/assets/safari-pinned-tab.svg" color="#0074d9">
		<link rel="shortcut icon" href="https://d3ward.github.io/assets/favicon.ico">
	<%} %>
	<meta name="theme-color" content="#ffffff">
	<link rel="manifest" href="https://d3ward.github.io/site.webmanifest">
	<meta name="msapplication-config" content="https://d3ward.github.io/browserconfig.xml">
	<meta name="msapplication-TileColor" content="#ffffff">

    <title><%= title %></title>
  	<meta name="author" content="Ursu Eduard Cornelius">
	<meta name="description" content="<%= description %>">
	<meta name="keywords" content="<%= keywords %>,d3ward,portfolio,projects,Eduard Ursu,Eduard Cornelius Ursu,GitHub,CSS,JS,Kiwi,HTML,Extensions,adblock">
	<!-- Google / Search Engine Tags -->
	<meta itemprop="name" content="<%= title %>">
	<meta itemprop="description" content="<%= description %>">
	<meta itemprop="image" content="https://d3ward.github.io/assets/preview_thumbnail.png">
	<!-- Open Graph Tags -->
	<meta property="og:url" content="<%= url %>">
	<meta property="og:type" content="website">
	<meta property="og:title" content="<%= title %>">
	<meta property="og:description" content="<%= description %>">
	<meta property="og:image" content="https://d3ward.github.io/assets/preview_thumbnail.png">

	<!-- Twitter Meta Tags -->
	<meta name="twitter:card" content="summary_large_image">
	<meta property="twitter:url" content="<%= url %>">
	<meta name="twitter:title" content="<%= title %>">
	<meta name="twitter:description" content="<%= description %>">
	<meta name="twitter:image" content="https://d3ward.github.io/assets/preview_thumbnail.png">
	<meta name="google-site-verification" content="xfQqUB8pw6Lm-7uMYXF5Rhrv85busGX1lfBrAGL90UI" />

	
</head>